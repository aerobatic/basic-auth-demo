{
  "_virtualApp": {
    "router": [
      {
        "module": "custom-errors",
        "options": {
          "errors": {
            "401": "errors/401.html"
          }
        }
      },
      {
        "module": "basic-auth",
        "path": "/protected",
        "options": {
          "username": "env:BASIC_AUTH_USERNAME",
          "password": "env:BASIC_AUTH_PASSWORD"
        }
      },
      {
        "module": "webpage"
      }
    ]
  }
}
